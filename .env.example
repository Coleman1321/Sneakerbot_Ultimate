# SneakerBot Ultimate - Environment Variables
# Copy this to .env and fill in your actual values

# Supabase Configuration (Cloud Database & Analytics)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# CAPTCHA Services
CAPTCHA_API_KEY=your_2captcha_api_key_here
ANTICAPTCHA_API_KEY=your_anticaptcha_api_key_here

# Notifications
DISCORD_WEBHOOK=https://discord.com/api/webhooks/your_webhook_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# AI Services (Optional - for auto-fix feature)
OPENAI_API_KEY=sk-your_openai_key_here
ANTHROPIC_API_KEY=sk-ant-your_anthropic_key_here

# Development Settings
DEBUG_MODE=False
DRY_RUN=True
HEADLESS=False

# Database
DATABASE_PATH=database/userdata.db
USE_SUPABASE=True

# Proxy Settings
USE_PROXIES=True
PROXY_FILE=config/proxies.txt

# Research Configuration
RESEARCH_MODE=True
ANALYTICS_ENABLED=True
TRACK_METRICS=True
